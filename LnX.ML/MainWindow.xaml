<Window x:Class="LnX.ML.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LnX.ML"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="400">
    <Grid>
        <Button x:Name="DNNBtn" Content="深度神经网络（DNN）" VerticalAlignment="Top" Height="30" Margin="10" Click="DNNBtn_Click" />
        <Button x:Name="CNNBtn" Content="卷积神经网络（CNN）" VerticalAlignment="Top" Height="30" Margin="10 50 10 10" Click="CNNBtn_Click" />
    </Grid>
</Window>
